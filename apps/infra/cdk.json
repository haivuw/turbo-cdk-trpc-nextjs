{
  "app": "npx ts-node -P tsconfig.json -r dotenv/config --prefer-ts-exts src/main.ts",
  "output": "cdk.out",
  "watch": {
    "include": ["src/**/*.ts", "test/**/*.ts"],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "node_modules"
    ]
  }
}
